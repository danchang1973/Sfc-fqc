procedure TForm1.Button1Click(Sender: TObject);
var
  s:string;
begin
  Edit2.Text:='';
  ADOQuery1.close;
  ADOQuery1.SQL.Clear;
  ADOQuery1.SQL.add('insert into SFCSNTA ( TBID, DT, SN, WN, TCID)');
  ADOQuery1.sql.add('values ( :TBID, :DT, :SN, :WN, :TCID) ');
  ADOQuery1.Parameters.ParamByName('TBID').Value:= ComboBox1.ItemIndex+101;
  ADOQuery1.Parameters.ParamByName('DT').Value:= STRTODATETIME(Label3.Caption);
  ADOQuery1.Parameters.ParamByName('SN').Value:= Edit2.Text;
  ADOQuery1.Parameters.ParamByName('WN').Value:= Edit3.Text;
  ADOQuery1.Parameters.ParamByName('TCID').Value:= ComboBox2.ItemIndex+201;
  ADOQuery1.ExecSQL;
  s:=ComboBox1.text + ' ' + Label3.Caption + ' ' + Edit2.Text + ' ' + Edit3.Text + ' ' + ComboBox2.text;
  memo1.Lines.Add(s);
end;

procedure TForm1.Button2Click(Sender: TObject);
begin
  MESSAGEBOX(FORM1.Handle,'箱號替換功能請小心使用!!','WARNING',MB_ICONWARNING);
  ADOQuery2.close;
  ADOQuery2.SQL.Clear;
  ADOQuery2.SQL.add('update SFCSNTD');
  ADOQuery2.SQL.add('set CN =:SN');
  ADOQuery2.SQL.add('where CN =:SN1');
  ADOQuery2.Parameters.ParamByName('SN').Value:= Edit5.Text;
  ADOQuery2.Parameters.ParamByName('SN1').Value:= Edit9.Text;
  ADOQuery2.ExecSQL;
  MESSAGEBOX(FORM1.Handle,'箱號替換完畢!!','WARNING',MB_ICONWARNING);
end;

procedure TForm1.Button3Click(Sender: TObject);
VAR
  asheet:variant;
  i:integer;
begin

  ADOQuery2.close;
  ADOQuery2.SQL.Clear;
  ADOQuery2.SQL.add('select * from SFCSNTH');
  ADOQuery2.SQL.add('where CN =:SN');
  ADOQuery2.SQL.add('and OK =1');
  ADOQuery2.SQL.add('order by SER');
  ADOQuery2.Parameters.ParamByName('SN').Value:= Edit9.Text;
  ADOQuery2.OPEN;
  IF ADOQuery2.Recordset.RecordCount=0 THEN
    BEGIN
      MESSAGEBOX(FORM1.Handle,'無整板序號資料!!','WARNING',MB_ICONWARNING);
    END;
  IF ADOQuery2.Recordset.RecordCount>0 THEN
    BEGIN
      ExcelApplication1.Visible[0]:=true;
      ExcelApplication1.Workbooks.Add(xlwbatworksheet,0);
      asheet:= ExcelApplication1.Worksheets.Item[1];
      asheet.cells[1,1].value:='板號';
      asheet.cells[1,2].value:='排序';
      asheet.cells[1,3].value:='序號';
      asheet.cells[1,4].value:='客戶序號';
      asheet.cells[1,5].value:='站別ID';
      asheet.cells[1,6].value:='工號';
      asheet.cells[1,7].value:='時間';

      for I := 1 to ADOQuery2.Recordset.RecordCount do
        begin
          asheet.cells[I+1,1].value:=ADOQuery2.FieldByName('CN').Value;
          asheet.cells[I+1,2].value:=ADOQuery2.FieldByName('SER').Value;
          asheet.cells[I+1,3].value:=ADOQuery2.FieldByName('SN').Value;
          asheet.cells[I+1,4].value:=ADOQuery2.FieldByName('CSN').Value;
          asheet.cells[I+1,5].value:=ADOQuery2.FieldByName('TB01').Value;
          asheet.cells[I+1,6].value:=ADOQuery2.FieldByName('WN').Value;
          asheet.cells[I+1,7].value:=ADOQuery2.FieldByName('DT').Value;
          ADOQuery2.next;
        end;
    end;
end;

procedure TForm1.Button4Click(Sender: TObject);
VAR
  asheet:variant;
  i,j:integer;
begin

  ADOQuery1.close;
  ADOQuery1.SQL.Clear;
  ADOQuery1.SQL.add('select * from SFCSNTH');
  ADOQuery1.SQL.add('where CN >=:SN1');
  ADOQuery1.SQL.add('and CN <=:SN2');
  ADOQuery1.SQL.add('order by CN,SER');
  ADOQuery1.Parameters.ParamByName('SN1').Value:= Edit9.Text;
  ADOQuery1.Parameters.ParamByName('SN2').Value:= Edit10.Text;
  ADOQuery1.OPEN;

  ExcelApplication1.Visible[0]:=true;
  ExcelApplication1.Workbooks.Add(xlwbatworksheet,0);
  asheet:= ExcelApplication1.Worksheets.Item[1];
  asheet.cells[1,1].value:='板號';
  asheet.cells[1,2].value:='排序';
  asheet.cells[1,3].value:='序號';
  asheet.cells[1,4].value:='客戶序號';
  asheet.cells[1,5].value:='站別ID';
  asheet.cells[1,6].value:='工號';
  asheet.cells[1,7].value:='時間';
  for I:=1 to ADOQuery1.recordcount do
    begin
      asheet.cells[I+1,1].value:=ADOQuery1.FieldByName('CN').Value;
      asheet.cells[I+1,2].value:=ADOQuery1.FieldByName('SER').Value;
      asheet.cells[I+1,3].value:=ADOQuery1.FieldByName('SN').Value;
      asheet.cells[I+1,4].value:=ADOQuery1.FieldByName('CSN').Value;
      asheet.cells[I+1,5].value:=ADOQuery1.FieldByName('TB01').Value;
      asheet.cells[I+1,6].value:=ADOQuery1.FieldByName('WN').Value;
      asheet.cells[I+1,7].value:=ADOQuery1.FieldByName('DT').Value;
      ADOQuery1.next;
    end;
end;

